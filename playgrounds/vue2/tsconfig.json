{
  "extends": "@mewcoder/configs/tsconfig.legacy.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "vite.config.ts"],
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"]
  },
  "vueCompilerOptions": {
    "target": 2.7
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
